{
    "manifest_version": 3,
    "name": "Vinted Message Tags",
    "version": "1.0",
    "description": "Add colored tags to messages on Vinted.",
    "permissions": ["storage", "activeTab"],
    "action": {
        // "default_popup": "popup.html",
        "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
        }
    },
    "content_scripts": [
        {
        "matches": ["https://www.vinted.com/inbox"],
        "js": ["contentScript.js"]
        }
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}